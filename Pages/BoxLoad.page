{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable",
			"Sections": [
				{
					"ProfileHeader": {
						"DetailImage": "sap-icon://shipping-status",
						"DetailImageIsCircular": false,
						"Headline": "Dispatch ID: {#Page:Main/#ClientData/HeaderDetail/DispatchId}",
						"Subheadline": "Transporter: {#Page:Main/#ClientData/HeaderDetail/Transporter}"
					},
					"_Type": "Section.Type.ProfileHeader",
					"_Name": "ProfileHeader",
					"Visible": true
				},
				{
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.SimpleProperty",
							"_Name": "ScanSticker",
							"IsEditable": true,
							"IsVisible": true,
							"Caption": "Scan",
							"PlaceHolder": "Scan Sticker",
							"OnValueChange": "/BoxDispatch/Rules/ScanBoxSuccess.js",
							"AlternateInput": "Barcode",
							"Enabled": false
						}
					],
					"Visible": true,
					"EmptySection": {
						"FooterVisible": false
					},
					"_Type": "Section.Type.FormCell",
					"_Name": "SectionFormCell0"
				},
				{
					"Header": {
						"Grid": {
							"Items": [
								{
									"LineBreakMode": "wordWrapping",
									"NumberOfLines": 1,
									"Text": "Invoice No",
									"TextAlignment": "center"
								},
								{
									"LineBreakMode": "wordWrapping",
									"NumberOfLines": 1,
									"Text": "Box ID",
									"TextAlignment": "center"
								}
							]
						},
						"_Name": "SectionGridHeader",
						"UseTopPadding": true
					},
					"Row": {
						"Items": [
							{
								"BindTo": "headline",
								"LineBreakMode": "wordWrapping",
								"NumberOfLines": 1,
								"Text": "{InvoiceNo}",
								"TextAlignment": "center"
							},
							{
								"BindTo": "statusImage",
								"LineBreakMode": "wordWrapping",
								"NumberOfLines": 1,
								"Text": "{BoxId}",
								"TextAlignment": "center"
							}
						],
						"Layout": {
							"ColumnWidth": [
								100,
								100
							]
						},
						"AccessoryType": "none"
					},
					"_Type": "Section.Type.GridTable",
					"DataSubscriptions": [
						"DispatchDetailsSet?$filter=DispatchId eq '{{#Page:Main/#ClientData/HeaderDetail/DispatchId}}'"
					],
					"Target": {
						"Service": "/BoxDispatch/Services/DISPATCH.service",
						"EntitySet": "DispatchDetailsSet",
						"QueryOptions": "$filter=DispatchId eq '{{#Page:Main/#ClientData/HeaderDetail/DispatchId}}'"
					},
					"_Name": "SectionGridTable",
					"Visible": true,
					"EmptySection": {
						"Caption": "No Box Scanned",
						"FooterVisible": false
					},
					"DataPaging": {
						"ShowLoadingIndicator": true,
						"LoadingIndicatorText": "",
						"PageSize": 100
					}
				}
			]
		}
	],
	"_Type": "Page",
	"_Name": "BoxLoad",
	"Caption": "Box Loading",
	"PrefersLargeCaption": false,
	"OnLoaded": "/BoxDispatch/Rules/onLoadingPageLoads.js",
	"OnUnloaded": "/BoxDispatch/Rules/onLoadingPageUnLoads.js",
	"ActionBar": {
		"Items": [
			{
				"_Name": "StartLoad",
				"Caption": "Start",
				"Position": "Right",
				"IsIconCircular": false,
				"Visible": true,
				"OnPress": "/BoxDispatch/Rules/StartLoad.js"
			},
			{
				"_Name": "StopLoad",
				"Caption": "Stop",
				"Position": "Right",
				"IsIconCircular": false,
				"Visible": false,
				"OnPress": "/BoxDispatch/Actions/StopLoadConfirmation.action"
			}
		],
		"_Name": "ActionBar1"
	}
}